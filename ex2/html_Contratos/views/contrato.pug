          |  #{c.dataCelebracaoContrato}
        li
          b Preço Contratual:
          |  #{c.precoContratual}
        li
          b Prazo de Execução:
          |  #{c.prazoExecucao}
        li
          b NIPC da Entidade Comunicante:
          | #{c.NIPC_entidade_comunicante}
        li
          b Entidade Comunicante:
          |  #{c.entidade_comunicante}
