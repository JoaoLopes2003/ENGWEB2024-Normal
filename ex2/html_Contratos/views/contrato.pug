extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1= titulo

    br
    .w3-container
      ul
        li
          b Id:
          |  #{c._id}
        li
          b Número do Anúncio:
          |  #{c.nAnuncio}
        li
          b Tipo de Procedimento:
          |  #{c.tipoprocedimento}
        li
          b Objecto do Contrato:
          |  #{c.objectoContrato}
        li
          b Data de Publicação:
          |  #{c.dataPublicacao}
